<launch>
    <!-- <node name="rviz viewer" pkg="rviz" type="rviz" output = "log" -->
    <node name="rviz_viewer" pkg="rviz" type="rviz" output = "log"
    args="-d $(find mapping_exp)/config/rviz_dense_config.rviz" />

    <arg name = "pkg_path"  default = "$(find mapping_exp)"  />
    <arg name = "config_file"  default = "/config/vins_euroc_para.yaml"  />
    <node name = "mapping_node"  pkg = "mapping_exp"  type = "mapping_node"  output = "screen" launch-prefix = "xterm -e gdb -ex run --args" args = "$(arg pkg_path) $(arg config_file)"/>

    <!-- 调用octomap_transform.launch文件 -->
    <!-- <include file="$(find octomap_server)/launch/octomap_transform.launch"/> -->
    <include file="$(find mapping_exp)/launch/octomap_transform.launch"/>
    
</launch>

